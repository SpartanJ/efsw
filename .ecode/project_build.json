{
  "linux-debug": {
    "build": [
      {
        "args": "--address-sanitizer --verbose gmake2",
        "command": "premake5",
        "working_dir": ""
      },
      {
        "args": "-j${nproc} -C make/linux",
        "command": "make",
        "working_dir": ""
      }
    ],
    "build_types": [],
    "clean": [
      {
        "args": " -C make/linux clean",
        "command": "make",
        "working_dir": ""
      }
    ],
    "config": {
      "clear_sys_env": false
    },
    "os": [
      "linux"
    ],
    "output_parser": {
      "config": {
        "preset": "generic",
        "relative_file_paths": true
      }
    },
    "run": [
      {
        "args": "test",
        "command": "efsw-test-debug",
        "name": "efsw-test-debug",
        "working_dir": "${project_root}/bin"
      }
    ]
  },
